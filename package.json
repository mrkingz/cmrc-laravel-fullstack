    "popper.js": "^1.12",
    "postcss": "^7.0.36",
    "postcss-css-variables": "^0.9.0",
    "vue": "^2.5.7",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-image": "^10.2.0",
